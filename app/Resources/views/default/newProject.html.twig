{% extends 'base.html.twig' %}

{% block body %}
    {{ include('partial/header.html.twig') }}

    <div class="container">
        <div class="row">
            <div class="col-md-7 col-sm-10 col-xs-12">
                <h1>New Project</h1>

                {{ form_start(form) }}
                {{ form_widget(form) }}

                <div class="alert alert-info">
                    Note that you can select multiple files holding CTRL keyboard button!
                </div>

                <input type="submit" class="btn btn-primary" value="Upload" />
                {{ form_end(form) }}

            </div>
        </div>
    </div>
    
{% endblock %}
